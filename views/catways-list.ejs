<!-- liste des catways --->
<%- include("partials/header") %>

<h2>Liste des Catways</h2>
<% if (typeof query !== "undefined" && query.success) { %>
<div class="alert alert-success"><%= query.success %></div>
<% } %> <% if (typeof query !== "undefined" && query.error) { %>
<div class="alert alert-danger"><%= query.error %></div>
<% } %>

<table border="1" cellpadding="8">
  <tr>
    <th>ID</th>
    <th>Numéro</th>
    <th>Type</th>
    <th>État</th>
    <th>Détails</th>
  </tr>

  <% catways.forEach(c => { %>
  <tr>
    <td><%= c._id %></td>
    <td><%= c.catwayNumber %></td>
    <td><%= c.type %></td>
    <td><%= c.catwayState %></td>
    <td><a href="/dashboard/view-catway?id=<%= c._id %>">Voir</a></td>
  </tr>
  <% }) %>
</table>
<br />
<a href="/dashboard">Retour au dashboard</a>
<%- include("partials/footer") %>
